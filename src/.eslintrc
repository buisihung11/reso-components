{
  "env": {
    "jest": true
  },
  "plugins": ["prettier", "@typescript-eslint"],
  "extends": ["react-app", "prettier"],
  "prettier/prettier": [
    2,
    {
      "singleQuote": true,
      "jsxSingleQuote": true,
      "semi": false,
      "tabWidth": 2,
      "bracketSpacing": true,
      "jsxBracketSameLine": false,
      "arrowParens": "always",
      "trailingComma": "none"
    }
  ]
}
